{
    "version": 2,
    "name": "ecomerce",
    "builds": [
        {
            "src": "Ecomerce/*",
            "use": "@vercel/static-build"
        },
        {
            "src": "actual-backend/*",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "actual-backend/$1"
        },
        {
            "src": "/(.*)",
            "dest": "Ecomerce/$1"
        }
    ]
}